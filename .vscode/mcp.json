{
    "servers": {
      "chakra-ui": {
        "command": "npx",
        "args": ["-y", "@chakra-ui/react-mcp"]
      },
      "supabase": {
        "command": "cmd",
        "args": [
          "/c",
          "npx",
          "-y",
          "@supabase/mcp-server-supabase",
          "--read-only",
          "--project-ref=<project-ref>"
        ],
        "env": {
          "SUPABASE_ACCESS_TOKEN": "process.env.SUPABASE_ACCESS_TOKEN"
        }
      }
    }
}
